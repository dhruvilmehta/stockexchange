generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Prices {
  id          String   @id @unique @default(uuid())
  companyName String
  time        DateTime @default(now())
  price       String

  @@unique([companyName, time])
  @@index([companyName])
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  token    String?
}
